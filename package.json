{
  "name": "ai_studio",
  "version": "1.0.0",
  "description": "The Revolution That Codes Itself",
  "author": "Matin",
  "private": true,
  "main": "src-electron/electron.js",
  "dependencies": {
    "@dhiwise/component-tagger": "^1.0.9",
    "@monaco-editor/react": "^4.7.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.6.0",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dgram": "^1.0.1",
    "dotenv": "^16.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^5.1.0",
    "framer-motion": "^10.16.4",
    "i18next": "^25.2.1",
    "i18next-http-backend": "^3.0.2",
    "localtunnel": "^2.0.2",
    "lucide-react": "^0.484.0",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "portfinder": "^1.0.37",
    "react": "^18.2.0",
    "react-arborist": "^3.4.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.5.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "6.0.2",
    "react-router-hash-link": "^2.4.3",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.2",
    "redux": "^5.0.1",
    "remark-gfm": "^4.0.1",
    "systeminformation": "^5.27.6",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build --sourcemap",
    "electron:start": "concurrently \"npm run start\" \"wait-on http://localhost:5173 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "dist": "npm run build && electron-builder"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.2",
    "concurrently": "^9.2.0",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "electron-updater": "^6.6.2",
    "postcss": "^8.5.6",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "3.4.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "3.6.0",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.matin.aistudio",
    "productName": "AI Studio",
    "copyright": "Copyright Â© 2025 ${author}",
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    },
    "files": [
      "build/**/*",
      "src-electron/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "resources/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "resources/icon.ico",
      "uninstallerIcon": "resources/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "resources/icon.icns",
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "resources/icon.png"
    }
  }
}